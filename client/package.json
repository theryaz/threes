{
  "name": "threes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service build && aws s3 sync --delete --acl public-read dist s3://play.threeswithfriends.com && aws cloudfront create-invalidation --distribution-id E3P2WX3OJLCJD3 --paths /index.html"
  },
  "dependencies": {
